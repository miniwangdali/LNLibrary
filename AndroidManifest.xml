<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="1" 
    android:versionName="1.0" 
    package="com.example.lnlibrary"
  	xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-sdk android:minSdkVersion="14" 
        android:targetSdkVersion="14" />
    <application android:theme="@style/AppTheme" 
        android:label="@string/app_name" 
        android:icon="@drawable/ic_launcher" 
        android:debuggable="true" android:allowBackup="true">
        <activity android:label="@string/app_name" 
            android:name="com.victor.lnlibrary.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" 
            android:name="com.victor.lnlibrary.MainActivity" />
        <activity android:label="@string/search_result" 
            android:name="com.victor.lnlibrary.SearchResultActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <meta-data android:name="android.app.searchable" 
                android:resource="@xml/searchable" />
        </activity>
        <activity android:label="@string/title_activity_reading" 
            android:name="com.victor.lnlibrary.ReadingActivity" />
    </application>
</manifest>
